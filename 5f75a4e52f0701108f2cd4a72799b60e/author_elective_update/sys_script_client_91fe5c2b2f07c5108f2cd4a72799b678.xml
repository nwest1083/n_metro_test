    <sys_script_client action="DELETE">
        <active>true</active>
        <applies_extended>false</applies_extended>
        <condition/>
        <description>On entry of the Employee in the Employee field populate Employee Number and Manager and Manager Number</description>
        <field>u_employee_name</field>
        <global>true</global>
        <isolate_script>true</isolate_script>
        <messages/>
        <name>Update User Information Section</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
   if (isLoading || newValue === '') {
   g_form.setValue('u_emp_id','');
   g_form.setValue('u_manager_name','');
   g_form.SetValue('u_department','');
      return;
   }

   //Type appropriate comment here, and begin script below
	var employee = g_form.getReference('u_employee_name');

	function setValues(employee) { //reference is passed into callback as first arguments

  g_form.setValue('u_emp_id',employee.employee_number);
  g_form.setValue('u_manager_name',employee.manager);
  g_form.setValue('u_department',employee.department);
	}  
}]]></script>
